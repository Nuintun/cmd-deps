{
  "name": "cmd-deps",
  "version": "3.0.1",
  "description": "Transform cmd and get cmd dependences.",
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/cmd-deps.git"
  },
  "bugs": {
    "url": "https://github.com/nuintun/cmd-deps/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "keywords": [
