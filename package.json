    "require",
    "common",
    "commonjs",
    "transform",
    "dependencies"
  ],
  "dependencies": {
    "acorn": "^7.3.1",
    "acorn-walk": "7.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "rollup": "^2.16.1"
